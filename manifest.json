{
  "manifest_version": 2,
  "name": "Google search faviconizer",
  "version": "0.1.0",
  "description": "Add site icon(favicon) in Google search results.",
  "author": "makitaaaaa",
  "applications": {
    "gecko": {
      "id": "gsf@makitaaaaa"
    }
  },
  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },
  "content_scripts": [{
    "matches": [
      "https://*/*"
    ],
    "include_globs": [
      "https://www.google.*/search*"
    ],
    "css": ["content.css"],
    "js": ["content.js"],
    "run_at": "document_end"
  }],
  "permissions": [
  ]
}